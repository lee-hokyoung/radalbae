doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    link(rel="apple-touch-icon" sizes="76x76" href="/nm/paper-dashboard-2/assets/img/apple-icon.png")
    link(rel="icon" type="image/png" href="/nm/paper-dashboard-2/assets/img/favicon.png")
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    title #{title}
    meta(content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no" name="viewport")
    // Fonts and icons
    link(href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet")
    link(href="https://fonts.googleapis.com/css?family=Jua|Gaegu&display=swap" rel="stylesheet")
    link(rel='stylesheet', href='/nm/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet', href='/nm/paper-dashboard-2/assets/css/bootstrap.min.css')
    link(rel='stylesheet', href='/nm/paper-dashboard-2/assets/css/paper-dashboard.min.css')
    // CSS Just for demo purpose, don't include it in your project
    link(href="/nm/paper-dashboard-2/assets/demo/demo.css" rel="stylesheet")
    link(href='/stylesheets/style.css', rel='stylesheet')
  body
    .wrapper
      include sidebar
      .main-panel
        include nav
        block content

    // Core JS Files
    script(src="/nm/jquery/dist/jquery.min.js")
    script(src="/nm/bootstrap/dist/js/bootstrap.bundle.min.js")
    //script(src="/nm/paper-dashboard-2/assets/js/core/jquery.min.js")
    //script(src="/nm/paper-dashboard-2/assets/js/core/popper.min.js")
    //script(src="/nm/paper-dashboard-2/assets/js/core/bootstrap.min.js")
    script(src="/nm/paper-dashboard-2/assets/js/plugins/perfect-scrollbar.jquery.min.js")
    // Google Maps Plugin
    //script(src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE")
    // Chart JS
    script(src="/nm/paper-dashboard-2/assets/js/plugins/chartjs.min.js")
    // Notifications Plugin
    script(src="/nm/paper-dashboard-2/assets/js/plugins/bootstrap-notify.js")
    // Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc
    script(src="/nm/paper-dashboard-2/assets/js/paper-dashboard.min.js" type="text/javascript")
    // Paper Dashboard DEMO methods, don't include it in your project!
    script(src="/nm/paper-dashboard-2/assets/demo/demo.js")
    block append_js